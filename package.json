  "main": "./dist/server.js",
  "scripts": {
    "docker:up": "docker compose up",
    "docker:down": "docker-compose down --rmi all --volumes",
    "db:push": "prisma db push",
    "db:push:test": "dotenv -e .env.test -e .env -- prisma db push",
    "db:generate": "prisma generate",
    "db:all": "npm run db:push && npm run db:push:test && npm run db:generate",
    "db:studio": "prisma studio",
    "typecheck": "biome check --write --unsafe",
    "build": "nest build",
    "dev": "npm run build && nest start --watch",
    "start": "nest start",
    "test": "jest",
    "pipe": "npm run typecheck && npm run build && npm run test",
    "gen:e2e": "plop e2e-test",
    "gen:service": "plop service-test"
  },
